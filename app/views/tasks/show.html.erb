
<h2><%= @task.title %></h2>
<p><%= @task.details %></p>

<% if @task.completed == true %>
  <input type="checkbox" checked disabled> This task is completed
<% else %>
  <input type="checkbox" disabled> This task is not completed yet
<% end %>




<%= link_to "Back to lisk", "/tasks"  %>
<%= link_to "Edit this task", task_edit_path %>

